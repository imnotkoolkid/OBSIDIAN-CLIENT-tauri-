{
  "productName": "Obsidian Client (pre release)",
  "version": "0.0.1",
  "identifier": "com.obsidian.kirka-client",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Obsidian Client (pre release)",
        "width": 1280,
        "height": 720,
        "minWidth": 800,
        "minHeight": 500,
        "center": true,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}


